    "import re\n",
    "\n",
    "def normalize_arabic(text):\n",
    "    text = re.sub(\"[إأآا]\", \"ا\", text)\n",
    "    text = re.sub(\"ى\", \"ي\", text)\n",
    "    text = re.sub(\"ؤ\", \"ء\", text)\n",
    "    text = re.sub(\"ئ\", \"ء\", text)\n",
    "    text = re.sub(\"ة\", \"ه\", text)\n",
    "    text = re.sub(\"گ\", \"ك\", text)\n",
    "    return text\n",
    "\n",
    "def replace_emojis_with_regex(text):\n",
    "    emojis = {\n",
    "        \"\\U0001F601\": \"فرح\",\n",
    "        \"\\U0001F602\": \"فرح\",\n",
    "        \"\\U0001F603\": \"فرح\",\n",
    "        \"\\U0001F604\": \"فرح\",\n",
    "        \"\\U0001F605\": \"حزن\",\n",
