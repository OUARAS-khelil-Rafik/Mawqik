    "        \"\\U0001F634\": \"حزن وفرح\",\n",
    "        \"\\U0001F635\": \"حزن\",\n",
    "        \"\\U0001F636\": \"حزن\",\n",
    "        \"\\U0001F637\": \"فرح\",\n",
