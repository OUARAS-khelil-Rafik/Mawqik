    "        \"\\U0001F622\": \"حزن\",\n",
    "        \"\\U0001F623\": \"فرح\",\n",
    "        \"\\U0001F624\": \"حزن\",\n",
    "        \"\\U0001F625\": \"حزن\",\n",
    "        \"\\U0001F626\": \"حزن\",\n",
